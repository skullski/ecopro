{
  "name": "fusion-starter",
  "private": true,
  "type": "module",
  "pkg": {
    "assets": [
      "dist/spa/*"
    ],
    "scripts": [
      "dist/server/**/*.js"
    ]
  },
  "scripts": {
    "dev": "concurrently \"pnpm:dev:*\"",
    "dev:client": "vite --port 5173",
    "dev:server": "nodemon --watch server --ignore logs/** --ignore server/logs/** --ignore **/*.log --exec tsx server/dev.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "vite build --config vite.config.server.ts",
    "start": "node run-migrations.js && node dist/server/node-build.cjs",
    "render:build": "npm run build",
    "test": "vitest --run",
    "format.fix": "prettier --write .",
    "typecheck": "tsc --pretty false",
    "migrate:rehash-passwords": "tsx server/scripts/rehash-passwords.ts",
    "extract:template-settings": "tsx scripts/extract-template-settings.ts",
    "e2e:smoke": "node scripts/e2e-smoke.js"
  },
